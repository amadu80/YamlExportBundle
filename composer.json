{
    "name": "psamatt/yaml-export-bundle",
    "type": "symfony-bundle",
    "description": "Yaml Exporter for use with Symfony2",
    "keywords": ["Yaml", "export", "dump", "symfony", "doctrine"],
    "homepage": "http://github.com/psamatt/YamlExportBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Goodwin",
            "email": "matt.goodwin491@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "Psamatt\\YamlExportBundle": "" }
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/console": ">=2.1,<3.0-dev",
        "symfony/framework-bundle": ">=2.1,<3.0-dev",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/dbal": "~2.2,>=2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3",
        "phpunit/dbunit": "1.2.*",
        "mockery/mockery": "dev-master",
    	"symfony/http-kernel": ">=2.1.0",
    	"symfony/dependency-injection": ">=2.1.0",
    	"symfony/yaml": "~2"
    },
    "minimum-stability": "dev",
    "target-dir": "Psamatt/YamlExportBundle"
}